{% if menu %}
	<div class="uk-navbar-dropdown">
		<ul class="uk-nav uk-navbar-dropdown-nav"></ul>
			{% for item in menu %}
				<li class="{{item.classes | join(' ')}}">
					<a href="{{item.get_link}}">
						{{item.title}}
					</a>
				</li>
			{% endfor %}
		</ul>
	</div>
{% endif %}